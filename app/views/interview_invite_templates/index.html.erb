<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All interview invites
    </h1>

    <a href="/interview_invites/new" class="btn btn-block btn-outline-secondary">
      Add a new interview invite
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Residency program
        </th>

        <th>
          Invite received date
        </th>

        <th>
          Invitee
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @interview_invites.each do |interview_invite| %>
      <tr>
        <!-- Display interview_invite.id start -->
        <td>
          <%= interview_invite.id %>
        </td>
        <!-- Display interview_invite.id end -->

        <!-- Display interview_invite.residency_program_id start -->
        <td>
          <%= interview_invite.residency_program.state %>
        </td>
        <!-- Display interview_invite.residency_program_id end -->

        <!-- Display interview_invite.invite_received_date start -->
        <td>
          <%= interview_invite.invite_received_date %>
        </td>
        <!-- Display interview_invite.invite_received_date end -->

        <!-- Display interview_invite.invitee_id start -->
        <td>
          <%= interview_invite.invitee_id %>
        </td>
        <!-- Display interview_invite.invitee_id end -->

        <!-- Display interview_invite.created_at start -->
        <td>
          <%= time_ago_in_words(interview_invite.created_at) %> ago
        </td>
        <!-- Display interview_invite.created_at end -->

        <!-- Display interview_invite.updated_at start -->
        <td>
          <%= time_ago_in_words(interview_invite.updated_at) %> ago
        </td>
        <!-- Display interview_invite.updated_at end -->

        <td>
          <a href="/interview_invites/<%= interview_invite.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
