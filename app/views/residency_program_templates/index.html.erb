<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All residency programs
    </h1>

    <a href="/residency_programs/new" class="btn btn-block btn-outline-secondary">
      Add a new residency program
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          State
        </th>

        <th>
          Program name
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @residency_programs.each do |residency_program| %>
      <tr>
        <!-- Display residency_program.id start -->
        <td>
          <%= residency_program.id %>
        </td>
        <!-- Display residency_program.id end -->

        <!-- Display residency_program.state start -->
        <td>
          <%= residency_program.state %>
        </td>
        <!-- Display residency_program.state end -->

        <!-- Display residency_program.program_name start -->
        <td>
          <%= residency_program.program_name %>
        </td>
        <!-- Display residency_program.program_name end -->

        <!-- Display residency_program.created_at start -->
        <td>
          <%= time_ago_in_words(residency_program.created_at) %> ago
        </td>
        <!-- Display residency_program.created_at end -->

        <!-- Display residency_program.updated_at start -->
        <td>
          <%= time_ago_in_words(residency_program.updated_at) %> ago
        </td>
        <!-- Display residency_program.updated_at end -->

        <td>
          <a href="/residency_programs/<%= residency_program.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
